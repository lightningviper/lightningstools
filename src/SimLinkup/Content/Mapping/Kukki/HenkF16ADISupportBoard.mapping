      <!-- HENK SDI HARDWARE SUPPORT MODULE - PITCH SDI BOARD - DIG_PWM_3 INPUT SIGNAL -->
      <Destination xsi:type="AnalogSignal">
        <Id>HenkSDI[0x30]__DIG_PWM_3</Id>
      </Destination>
      
    </SignalMapping>
    
  <!-- VERTICAL GS BAR SIGNAL FROM HENK ADI HARDWARE SUPPORT MODULE TO HENK SYNCHRO DRIVE INTERFACE BOARD  -->
    <SignalMapping>
      <!-- HENK ADI PRIMARY ADI HARDWARE SUPPORT MODULE VERTICAL GS BAR OUTPUT SIGNAL -->
