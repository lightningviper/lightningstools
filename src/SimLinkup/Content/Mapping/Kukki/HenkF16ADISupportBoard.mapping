      <!-- HENK ADI LOC FLAG INPUT SIGNAL-->
      <Destination xsi:type="DigitalSignal">
        <Id>HenkF16ADISupportBoard_LOC_Flag_From_Sim</Id>
      </Destination>
      
    </SignalMapping>

    <!-- OFF FLAG SIGNAL FROM SIM -->
    <SignalMapping>
      <!-- F4 OFF FLAG OUTPUT SIGNAL-->
      <Source xsi:type="DigitalSignal">
        <Id>F4_ADI__OFF_FLAG</Id>
      </Source>
      <!-- HENK ADI OFF FLAG INPUT SIGNAL-->
      <Destination xsi:type="DigitalSignal">
        <Id>HenkF16ADISupportBoard_OFF_Flag_From_Sim</Id>
      </Destination>
      
    </SignalMapping>
    

    <!-- "COMMAND BARS VISIBLE" SIGNAL FROM SIM -->
    <SignalMapping>
      <!-- F4 SHOW COMMAND BARS OUTPUT SIGNAL-->
      <Source xsi:type="DigitalSignal">
        <Id>F4_ADI__ILS_SHOW_COMMAND_BARS</Id>
      </Source>
      <!-- HENK ADI SHOW COMMAND BARS INPUT SIGNAL-->
      <Destination xsi:type="DigitalSignal">
        <Id>HenkF16ADISupportBoard_Command_Bars_Visible_Flag_From_Sim</Id>
      </Destination>
      
    </SignalMapping>
    
    <!-- RATE OF TURN INDICATOR SIGNAL FOM SIM -->
    <SignalMapping>
      <!-- F4 RATE OF TURN OUTPUT SIGNAL-->
      <Source xsi:type="AnalogSignal">
        <Id>F4_ADI__RATE_OF_TURN_INDICATOR_POSITION</Id>
      </Source>
      <!-- HENK ADI RATE OF TURN INPUT SIGNAL-->
      <Destination xsi:type="AnalogSignal">
        <Id>HenkF16ADISupportBoard_Rate_Of_Turn_Indicator_From_Sim</Id>
      </Destination>
      
    </SignalMapping>

    <!-- ***************************************************************************************************************************************-->
    <!-- MAPPINGS FROM HENK ADI SUPPORT BOARD HADRWARE SUPPORT MODULE OUTPUTS TO HENK SDI SYNCHRO DRIVE INTERFACE HARDWARE SUPPORT MODULE INPUTS-->
    <!-- ***************************************************************************************************************************************-->

            <!-- *******************************************************************************************************************-->
            <!--PITCH SDI-->
            <!-- *******************************************************************************************************************-->
    
    <!-- PITCH SIGNAL FROM HENK ADI HARDWARE SUPPORT MODULE TO HENK SYNCHRO DRIVE INTERFACE BOARD-->
    <SignalMapping>
      <!-- HENK ADI PRIMARY ADI HARDWARE SUPPORT MODULE PITCH OUTPUT SIGNAL -->
      <Source xsi:type="AnalogSignal">
        <Id>HenkF16ADISupportBoard_Pitch_To_SDI</Id>
      </Source>
      <!-- HENK SDI HARDWARE SUPPORT MODULE - PITCH SDI BOARD - SYNCHRO POSITION INPUT SIGNAL -->
      <Destination xsi:type="AnalogSignal">
        <Id>HenkSDI[0x30]__Synchro_Position</Id>
      </Destination>
      
    </SignalMapping>

    <!-- PITCH AND ROLL SERVO ENABLE SIGNAL FROM HENK ADI HARDWARE SUPPORT MODULE TO HENK SYNCHRO DRIVE INTERFACE BOARD  -->
    <SignalMapping>
      <!-- HENK ADI PRIMARY ADI HARDWARE SUPPORT MODULE PITCH AND ROLL SERVO ENABLE OUTPUT SIGNAL-->
      <Source xsi:type="DigitalSignal">
        <Id>HenkF16ADISupportBoard_ENABLE_PITCH_AND_ROLL_To_SDI</Id>
      </Source>
      <!-- HENK SDI HARDWARE SUPPORT MODULE - PITCH SDI BOARD - DIG_PWM_1 INPUT SIGNAL-->
      <Destination xsi:type="DigitalSignal">
        <Id>HenkSDI[0x30]__DIG_PWM_1</Id>
      </Destination>
      
    </SignalMapping>

    <!-- GLIDE SLOPE INDICATORS POWER ON/OFF SIGNAL FROM HENK ADI HARDWARE SUPPORT MODULE TO HENK SYNCHRO DRIVE INTERFACE BOARD  -->
