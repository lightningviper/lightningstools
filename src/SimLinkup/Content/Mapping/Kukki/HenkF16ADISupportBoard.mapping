      <Destination xsi:type="AnalogSignal">
        <Id>HenkSDI[0x30]__DIG_PWM_4</Id>
      </Destination>
      
    </SignalMapping>

    
    
          <!-- *******************************************************************************************************************-->    
          <!--ROLL SDI-->
          <!-- *******************************************************************************************************************-->
    <!-- ROLL SIGNAL FROM HENK ADI HARDWARE SUPPORT MODULE TO HENK SYNCHRO DRIVE INTERFACE BOARD-->
