        <Id>HenkF16ADISupportBoard_LOC_Flag_To_SDI</Id>
      </Source>
      <!-- HENK SDI HARDWARE SUPPORT MODULE - ROLL SDI BOARD - DIG_PWM_3 INPUT SIGNAL-->
      <Destination xsi:type="DigitalSignal">
        <Id>HenkSDI[0x32]__DIG_PWM_3</Id>
      </Destination>
      
    </SignalMapping>

    <!-- AUX FLAG SIGNAL FROM HENK ADI HARDWARE SUPPORT MODULE TO HENK SYNCHRO DRIVE INTERFACE BOARD  -->
    <SignalMapping>
      <!-- HENK ADI PRIMARY ADI HARDWARE SUPPORT MODULE AUX FLAG OUTPUT SIGNAL-->
      <Source xsi:type="DigitalSignal">
