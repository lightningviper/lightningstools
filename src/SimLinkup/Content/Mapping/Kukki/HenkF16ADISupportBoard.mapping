<?xml version="1.0"?>
<MappingProfile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <SignalMappings>

    <!-- **************************************************************************************** -->
    <!-- MAPPINGS FROM F4 SIM VALUES TO HENK F16 ADI SUPPORT BOARD HARDWARE SUPPORT MODULE INPUTS -->
    <!-- **************************************************************************************** -->
    <!-- PITCH DEGREES SIGNAL FROM SIM -->
    <SignalMapping>
      <!-- F4 ADI PITCH DEGREES OUTPUT SIGNAL-->
      <Source xsi:type="AnalogSignal">
        <Id>F4_ADI__PITCH_DEGREES</Id>
      </Source>
      <!-- HENK ADI PITCH DEGREES INPUT SIGNAL-->
      <Destination xsi:type="AnalogSignal">
        <Id>HenkF16ADISupportBoard_Pitch_From_Sim</Id>
      </Destination>
      
    </SignalMapping>

    <!-- ROLL DEGREES SIGNAL FROM SIM -->
    <SignalMapping>
      <!-- F4 ADI ROLL DEGREES OUTPUT SIGNAL-->
      <Source xsi:type="AnalogSignal">
        <Id>F4_ADI__ROLL_DEGREES</Id>
      </Source>
      <!-- HENK ADI ROLL DEGREES INPUT SIGNAL -->
      <Destination xsi:type="AnalogSignal">
        <Id>HenkF16ADISupportBoard_Roll_From_Sim</Id>
      </Destination>
      
    </SignalMapping>
    
    <!-- HORIZONTAL COMMAND BAR SIGNAL FROM SIM  -->
    <SignalMapping>
      <!-- F4 HORIZONTAL COMMAND BAR OUTPUT SIGNAL -->
      <Source xsi:type="AnalogSignal">
        <Id>F4_ADI__ILS_HORIZONTAL_BAR_POSITION</Id>
      </Source>
      <!-- HENK ADI HORIZONTAL COMMAND BAR INPUT SIGNAL-->
      <Destination xsi:type="AnalogSignal">
        <Id>HenkF16ADISupportBoard_Horizontal_Command_Bar_From_Sim</Id>
      </Destination>
      
    </SignalMapping>
    
    <!-- VERTICAL COMMAND BAR SIGNAL FROM SIM -->
    <SignalMapping>
      <!-- F4 VERTICAL COMMAND BAR OUTPUT SIGNAL -->
