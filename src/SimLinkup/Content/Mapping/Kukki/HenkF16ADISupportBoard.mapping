        <Id>HenkF16ADISupportBoard_GS_POWER_To_SDI</Id>
      </Source>
      <!-- HENK SDI HARDWARE SUPPORT MODULE - PITCH SDI BOARD - DIG_PWM_2 INPUT SIGNAL-->
      <Destination xsi:type="DigitalSignal">
        <Id>HenkSDI[0x30]__DIG_PWM_2</Id>
      </Destination>
      
    </SignalMapping>
    
    <!-- HORIZONTAL GS BAR SIGNAL FROM HENK ADI HARDWARE SUPPORT MODULE TO HENK SYNCHRO DRIVE INTERFACE BOARD -->
