      <!-- HENK SDI HARDWARE SUPPORT MODULE - ROLL SDI BOARD - SYNCHRO POSITION INPUT SIGNAL-->
      <Destination xsi:type="AnalogSignal">
        <Id>HenkSDI[0x32]__Synchro_Position</Id>
      </Destination>
      
    </SignalMapping>

    <!-- GS FLAG SIGNAL FROM HENK ADI HARDWARE SUPPORT MODULE TO HENK SYNCHRO DRIVE INTERFACE BOARD  -->
    <SignalMapping>
      <!-- HENK ADI PRIMARY ADI HARDWARE SUPPORT MODULE GS FLAG OUTPUT SIGNAL-->
