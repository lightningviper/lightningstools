      <Destination xsi:type="DigitalSignal">
        <Id>HenkSDI[0x32]__DIG_PWM_2</Id>
      </Destination>
      
    </SignalMapping>
    
    <!-- LOC FLAG SIGNAL FROM HENK ADI HARDWARE SUPPORT MODULE TO HENK SYNCHRO DRIVE INTERFACE BOARD  -->
