      <!-- HENK ADI VERTICAL COMMAND BAR INPUT SIGNAL-->
      <Destination xsi:type="AnalogSignal">
        <Id>HenkF16ADISupportBoard_Vertical_Command_Bar_From_Sim</Id>
      </Destination>
      
    </SignalMapping>

    <!-- AUX FLAG SIGNAL FROM SIM -->
    <SignalMapping>
      <!-- F4 AUX FLAG OUTPUT SIGNAL-->
      <Source xsi:type="DigitalSignal">
        <Id>F4_ADI__AUX_FLAG</Id>
      </Source>
      <!-- HENK ADI AUX FLAG INPUT SIGNAL-->
      <Destination xsi:type="DigitalSignal">
        <Id>HenkF16ADISupportBoard_AUX_Flag_From_Sim</Id>
      </Destination>
      
    </SignalMapping>

    <!-- GS FLAG SIGNAL FROM SIM -->
    <SignalMapping>
      <!-- F4 GS FLAG OUTPUT SIGNAL-->
      <Source xsi:type="DigitalSignal">
        <Id>F4_ADI__GS_FLAG</Id>
